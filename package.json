{
  "name": "@prageeths/simple-i18n",
  "version": "1.3.0",
  "description": "Simple i18n library",
  "license": "Apache-2.0",
  "keywords": [
    "i18n",
    "l10n",
    "internationalisation",
    "localisation"
  ],
  "author": {
    "name": "Prageeth Silva",
    "email": "contact@prageethsilva.com"
  },
